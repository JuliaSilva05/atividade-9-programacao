//atv 9 q 5
#include <stdio.h>
#include <stdlib.h>
int pell(int n)
{
    if (n==0||n==1){
        return n;
    }
    else if (n>1){
        return 2*pell(n-1) + pell(n-2);
    }
    
}
int main(){
    int n;
    int res;
    printf("Digite um n√∫mero\n");
    scanf("%d",&n);
    res = pell(n);
    printf("resultado: %d",res);
    
 }
